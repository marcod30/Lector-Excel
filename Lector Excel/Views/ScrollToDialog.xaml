<Window x:Class="Reader_347.ScrollToDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:local="clr-namespace:Reader_347"
        mc:Ignorable="d"
        Title="Desplazarse a declarado" Height="150" Width="300" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Canvas>
        <Label Canvas.Left="15" Canvas.Top="50"> Ir al declarado:</Label>
        <xctk:IntegerUpDown Name="nud_MainNumericUpDown" Canvas.Left="110" Canvas.Top="50" Height="25" Width="100" Value="1" Minimum="1" ValueChanged="Nud_MainNumericUpDown_ValueChanged"/>
    </Canvas>
</Window>
