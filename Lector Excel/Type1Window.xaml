<Window x:Class="Lector_Excel.Type1Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lector_Excel"
        mc:Ignorable="d"
        Title="Type1Window" Height="450" Width="800" ResizeMode="NoResize">
    <DockPanel Margin="5">
        <Label DockPanel.Dock="Top">
            Ejercicio
        </Label>
        <TextBox DockPanel.Dock="Top" MaxLength="4" Height="30">
            
        </TextBox>
        <Label DockPanel.Dock="Top">
            Nombre y apellidos del declarante
        </Label>
        <TextBox DockPanel.Dock="Top" MaxLength="40" Height="30">
            
        </TextBox>
        <Label DockPanel.Dock="Top">
            NIF del declarante
        </Label>
        <TextBox DockPanel.Dock="Top" MaxLength="9" Height="30">
            
        </TextBox>
        <Label DockPanel.Dock="Top">
            Número de personas y entidades
        </Label>
        <TextBox DockPanel.Dock="Top" MaxLength="4" Height="30">
            
        </TextBox>
        <Label DockPanel.Dock="Top">
            Importe total
        </Label>
        <TextBox DockPanel.Dock="Top" Height="30" VerticalAlignment="Top">

        </TextBox>
    </DockPanel>
</Window>
